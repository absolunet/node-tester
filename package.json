    "@absolunet/eslint-config-react": "3.0.0-beta.1",
    "@absolunet/eslint-config-test": "3.0.0-beta.1",
    "@absolunet/fss": "^1.7.0",
    "@absolunet/joi": "^0.3.0",
    "@absolunet/terminal": "^3.1.0",
