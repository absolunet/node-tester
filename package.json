{
  "name": "@absolunet/tester",
  "version": "4.2.0",
  "description": "Test suite for Node.js projects",
  "definition": "",
  "homepage": "https://documentation.absolunet.com/node-tester",
  "author": {
    "name": "Absolunet",
    "url": "https://absolunet.com"
  },
  "keywords": [
    "eclint",
    "editorconfig",
    "eslint",
    "jest",
    "json",
    "keepachangelog",
    "lint",
    "linter",
    "stylelint",
    "yaml"
  ],
  "license": "MIT",
  "repository": {
    "url": "git://github.com/absolunet/node-tester.git",
    "type": "git"
